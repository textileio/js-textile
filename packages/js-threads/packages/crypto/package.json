{
  "name": "@textile/threads-crypto",
  "version": "0.0.5",
  "main": "dist/index",
  "types": "dist/index",
  "files": [
    "dist/**/!(*.spec).js?(.map)",
    "dist/**/!(*.spec).d.ts"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "prepublishOnly": "npm run build",
    "prepare": "npm run build:node",
    "prebuild": "npm run clean",
    "build": "npm run build:protos && npm run build:node && npm run build:browser",
    "build:node": "npm run compile",
    "build:protos": "npx pbjs src/keys.proto --ts src/proto.keys.ts",
    "build:browser": "webpack --mode production --config ../../webpack.config.js",
    "compile": "tsc -b tsconfig.json",
    "clean": "rimraf ./dist ./tsconfig.tsbuildinfo"
  },
  "license": "MIT",
  "repository": "github:textileio/js-threads",
  "devDependencies": {
    "@types/multibase": "^0.6.0",
    "@types/varint": "^5.0.0",
    "rimraf": "^3.0.0",
    "typescript": "^3.7.2"
  },
  "dependencies": {
    "@consento/sync-randombytes": "^1.0.5",
    "@types/google-protobuf": "^3.7.2",
    "fast-sha256": "^1.3.0",
    "google-protobuf": "3.11.4",
    "micro-aes-gcm": "^0.2.0",
    "multibase": "^1.0.1",
    "noble-ed25519": "^1.0.0",
    "varint": "^5.0.0"
  },
  "gitHead": "40d7c6d2c3539aa273c112a6f918983ef704c179"
}
