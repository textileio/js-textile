{
  "name": "@textile/hub-threads-client",
  "version": "0.2.2",
  "description": "web-gRPC wrapper client for communicating with Threads APIs on the Hub.",
  "main": "dist/index",
  "types": "dist/index",
  "files": [
    "dist/**/!(*.spec).js?(.map)",
    "dist/**/!(*.spec).d.ts"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "prepublishOnly": "npm run build",
    "prepare": "npm run build",
    "prebuild": "npm run clean",
    "build": "tsc -b tsconfig.json",
    "clean": "rimraf ./dist ./tsconfig.tsbuildinfo",
    "test:browser": "polendina --cleanup --timeout 60 src/**/*.spec.ts --webpack-config ../../webpack.test.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/textileio/js-hub.git"
  },
  "bugs": {
    "url": "https://github.com/textileio/js-hub/issues"
  },
  "author": "Textile",
  "license": "MIT",
  "dependencies": {
    "@improbable-eng/grpc-web": "^0.12.0",
    "@textile/context": "^0.6.3",
    "@textile/security": "^0.2.3",
    "@textile/threads-client": "^0.8.3",
    "@textile/threads-id": "^0.1.12",
    "@textile/users-grpc": "^1.0.13",
    "cids": "^0.8.0",
    "loglevel": "^1.6.8",
    "multihashes": "0.4.19"
  },
  "devDependencies": {
    "@textile/hub-grpc": "^1.0.13",
    "@textile/threads-core": "^0.1.32",
    "axios": "^0.19.2",
    "delay": "^4.3.0",
    "fast-sha256": "^1.3.0",
    "multibase": "^0.7.0",
    "rimraf": "^3.0.2",
    "typescript": "^3.9.3"
  }
}
