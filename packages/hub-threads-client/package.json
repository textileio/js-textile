{
  "name": "@textile/hub-threads-client",
  "version": "3.0.0",
  "description": "web-gRPC wrapper client for communicating with Threads APIs on the Hub.",
  "main": "dist/index",
  "types": "dist/index",
  "files": [
    "dist/**/!(*.spec).js?(.map)",
    "dist/**/!(*.spec).d.ts"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "prepublishOnly": "npm run build",
    "prepare": "npm run build",
    "prebuild": "npm run clean",
    "build": "tsc -b tsconfig.json",
    "clean": "rimraf ./dist ./tsconfig.tsbuildinfo",
    "test:browser": "polendina --cleanup --timeout 60 src/**/*.spec.ts --webpack-config ../../webpack.test.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/textileio/js-textile.git"
  },
  "bugs": {
    "url": "https://github.com/textileio/js-textile/issues"
  },
  "author": "Textile",
  "license": "MIT",
  "dependencies": {
    "@improbable-eng/grpc-web": "^0.13.0",
    "@textile/context": "^0.9.2",
    "@textile/hub-grpc": "2.2.1",
    "@textile/security": "^0.6.2",
    "@textile/threads-client": "^1.3.2",
    "@textile/threads-id": "^0.3.1",
    "@textile/users-grpc": "2.2.1",
    "loglevel": "^1.7.0"
  },
  "devDependencies": {
    "@textile/crypto": "^2.0.0",
    "axios": "^0.20.0",
    "delay": "^4.4.0",
    "fast-sha256": "^1.3.0",
    "multibase": "^3.1.0",
    "rimraf": "^3.0.2",
    "typescript": "^3.9.3"
  },
  "references": [
    {
      "path": "../crypto"
    }
  ]
}