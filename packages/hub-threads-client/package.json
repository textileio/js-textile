    "@textile/threads-client": "^2.1.1",
    "@textile/threads-id": "^0.5.1",
    "@textile/users-grpc": "2.6.0",
    "loglevel": "^1.7.0"
  },
  "devDependencies": {
    "@textile/crypto": "^4.1.1",
    "@textile/testing": "^0.0.2"
