{
  "name": "@textile/users",
  "version": "0.0.1",
  "description": "web-gRPC wrapper client for managing Textile Users.",
  "main": "dist/index",
  "types": "dist/index",
  "files": [
    "dist/**/!(*.spec).js?(.map)",
    "dist/**/!(*.spec).d.ts"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "prepublishOnly": "npm run build",
    "prepare": "npm run build",
    "prebuild": "npm run clean",
    "build": "tsc -b tsconfig.json",
    "clean": "rimraf ./dist ./tsconfig.tsbuildinfo",
    "test:browser": "polendina --cleanup --timeout 60 src/**/*.spec.ts --webpack-config ../../webpack.test.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/textileio/js-hub.git"
  },
  "bugs": {
    "url": "https://github.com/textileio/js-hub/issues"
  },
  "author": "Textile",
  "license": "MIT",
  "dependencies": {
    "@improbable-eng/grpc-web": "^0.12.0",
    "@textile/buckets-grpc": "^1.0.14",
    "@textile/context": "^0.6.4",
    "@textile/grpc-authentication": "^0.0.1",
    "@textile/grpc-connection": "^0.0.1",
    "@textile/grpc-transport": "^0.0.2",
    "@textile/hub-threads-client": "^0.3.1",
    "@textile/security": "^0.2.4",
    "@textile/threads-client": "^0.8.4",
    "@textile/threads-core": "^0.1.33",
    "@textile/threads-id": "^0.1.13",
    "@textile/users-grpc": "^1.0.14",
    "@types/next-tick": "^1.0.0",
    "cids": "^0.8.0",
    "event-iterator": "^2.0.0",
    "loglevel": "^1.6.8",
    "next-tick": "^1.1.0"
  },
  "devDependencies": {
    "@textile/hub-grpc": "^1.0.14",
    "@textile/threads-client": "^0.8.4",
    "axios": "^0.19.2",
    "delay": "^4.3.0",
    "fast-sha256": "^1.3.0",
    "multibase": "^0.7.0",
    "rimraf": "^3.0.2",
    "typescript": "^3.9.3"
  },
  "references": [
    {
      "path": "../hub-threads-client"
    },
    {
      "path": "../grpc-authentication"
    },
    {
      "path": "../grpc-connection"
    }
  ]
}
