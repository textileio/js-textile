{
  "name": "@textile/grpc-authentication",
  "version": "0.2.1",
  "description": "web-gRPC state manager for grpc-connection.",
  "main": "dist/index",
  "types": "dist/index",
  "files": [
    "dist/**/!(*.spec).js?(.map)",
    "dist/**/!(*.spec).d.ts"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "prepublishOnly": "npm run build",
    "prepare": "npm run build",
    "prebuild": "npm run clean",
    "build": "tsc -b tsconfig.json",
    "clean": "rimraf ./dist ./tsconfig.tsbuildinfo"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/textileio/js-hub.git"
  },
  "bugs": {
    "url": "https://github.com/textileio/js-hub/issues"
  },
  "author": "Textile",
  "license": "MIT",
  "dependencies": {
    "@textile/context": "^0.7.0",
    "@textile/crypto": "^0.1.0",
    "@textile/grpc-connection": "^0.1.4-alpha.0",
    "@textile/hub-threads-client": "^0.5.0",
    "@textile/security": "^0.2.7"
  },
  "devDependencies": {
    "rimraf": "^3.0.2",
    "typescript": "^4.0.3"
  },
  "references": [
    {
      "path": "../hub-threads-client"
    },
    {
      "path": "../grpc-connection"
    },
    {
      "path": "../crypto"
    }
  ]
}
