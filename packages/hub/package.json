    "delay": "^4.4.0",
    "fast-sha256": "^1.3.0",
    "multibase": "^3.1.0",
    "rimraf": "^3.0.2",
    "typescript": "^3.9.3"
  },
  "references": [
    {
      "path": "../buckets"
    },
    {
      "path": "../users"
    },
