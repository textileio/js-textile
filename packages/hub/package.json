{
  "name": "@textile/hub",
  "version": "0.4.0",
  "description": "web-gRPC wrapper client for communicating with Textile Hub.",
  "main": "dist/index",
  "types": "dist/index",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "prepublishOnly": "npm run build",
    "prepare": "npm run build",
    "prebuild": "npm run clean",
    "build": "tsc -b tsconfig.json",
    "clean": "rimraf ./dist ./tsconfig.tsbuildinfo",
    "test:browser": "polendina --cleanup --timeout 60 src/**/*.spec.ts --webpack-config ../../webpack.test.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/textileio/js-hub.git"
  },
  "bugs": {
    "url": "https://github.com/textileio/js-hub/issues"
  },
  "author": "Textile",
  "license": "MIT",
  "dependencies": {
    "@improbable-eng/grpc-web": "^0.12.0",
    "@textile/buckets": "^0.5.0",
    "@textile/hub-threads-client": "^0.3.0",
    "@textile/security": "^0.2.4",
    "@textile/threads-client": "^0.8.4",
    "@textile/threads-core": "^0.1.33",
    "@textile/threads-database": "^0.6.5",
    "@textile/threads-id": "^0.1.13",
    "@textile/users-grpc": "^1.0.13",
    "cids": "^0.8.0",
    "loglevel": "^1.6.8",
    "multihashes": "0.4.19"
  },
  "devDependencies": {
    "@textile/hub-grpc": "^1.0.13",
    "@textile/threads-core": "^0.1.33",
    "axios": "^0.19.2",
    "delay": "^4.3.0",
    "fast-sha256": "^1.3.0",
    "multibase": "^0.7.0",
    "rimraf": "^3.0.2",
    "typescript": "^3.9.3"
  },
  "references": [
    {
      "path": "../buckets"
    },
    {
      "path": "../hub-threads-client"
    }
  ]
}
