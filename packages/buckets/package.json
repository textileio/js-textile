{
  "name": "@textile/buckets",
  "version": "0.2.4",
  "description": "web-gRPC wrapper client for managing Textile Buckets.",
  "main": "dist/index",
  "types": "dist/index",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "prepublishOnly": "npm run build",
    "prepare": "npm run build:node",
    "prebuild": "npm run clean",
    "build": "npm run build:node && npm run build:browser",
    "build:node": "npm run compile",
    "build:browser": "webpack --mode production --config ../../webpack.config.js",
    "compile": "tsc -b tsconfig.json",
    "clean": "rimraf ./dist ./tsconfig.tsbuildinfo",
    "test:browser": "polendina --cleanup --timeout 60 src/**/*.spec.ts --webpack-config ../../webpack.test.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/textileio/js-hub.git"
  },
  "bugs": {
    "url": "https://github.com/textileio/js-hub/issues"
  },
  "author": "Textile",
  "license": "MIT",
  "dependencies": {
    "@improbable-eng/grpc-web": "^0.12.0",
    "@textile/buckets-grpc": "^1.0.8",
    "@textile/context": "^0.4.7",
    "@textile/security": "^0.1.4",
    "cids": "^0.8.0",
    "loglevel": "^1.6.8",
    "queueable": "^4.1.4"
  },
  "devDependencies": {
    "@textile/hub-grpc": "^1.0.8",
    "@textile/threads-client": "^0.6.13",
    "@textile/threads-core": "^0.1.24",
    "@textile/threads-id": "^0.1.6",
    "axios": "^0.19.2",
    "delay": "^4.3.0",
    "fast-sha256": "^1.3.0",
    "multibase": "^0.7.0",
    "rimraf": "^3.0.2",
    "typescript": "^3.9.3"
  }
}
