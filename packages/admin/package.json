{
  "name": "@textile/hub-admin",
  "version": "0.0.0",
  "description": "web-gRPC wrapper client for managing Textile Hub admin.",
  "main": "dist/cjs/index.js",
  "types": "dist/cjs/index.d.ts",
  "module": "dist/esm/index.js",
