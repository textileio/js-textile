    "@textile/grpc-transport": "^0.4.0",
    "@textile/hub-grpc": "^2.6.0",
    "loglevel": "^1.7.0"
  },
  "devDependencies": {
    "@textile/crypto": "^4.1.1",
    "@textile/testing": "^0.0.3"
  }
