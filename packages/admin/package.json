    "@textile/grpc-connection": "^2.2.0",
    "@textile/grpc-transport": "^0.3.0",
    "@textile/hub-grpc": "^2.5.0",
    "loglevel": "^1.7.0"
  },
  "devDependencies": {
    "@textile/crypto": "^2.1.1",
    "axios": "^0.20.0"
  }
